{
	"manifest_version": 3,
	"name": "Rettiwt-Auth Helper",
	"version": "1.0",
	"description": "Helps authenticating Rettiwt-API",
	"permissions": ["cookies", "storage"],
	"host_permissions": ["*://x.com/*"],
	"incognito": "split",
	"background": {
		"scripts": ["js/background.js"]
	},
	"action": {
		"default_popup": "pages/popup.html"
	}
}
